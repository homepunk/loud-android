<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleTabLayout">
        <attr name="bubble_buttonGap" format="integer|dimension"/>
        <attr name="bubble_defaultColor" format="color"/>
        <attr name="bubble_highlightedColor" format="color"/>
        <attr name="bubble_textAllCaps" format="boolean"/>
        <attr name="bubble_tabFixedWidth" format="boolean"/>
        <attr name="bubble_tabVerticalMargin" format="integer|dimension"/>
        <attr name="bubble_startMargin" format="integer|dimension"/>
        <attr name="bubble_iconWidth" format="integer|dimension"/>
        <attr name="bubble_iconHeight" format="integer|dimension"/>
    </declare-styleable>

    <declare-styleable name="ExpandableHeader">
        <attr name="el_title" format="string"/>
        <attr name="el_expanded" format="boolean"/>
        <attr name="el_headerTint" format="color"/>
        <attr name="el_headerTitleStartMargin" format="integer|dimension"/>
        <attr name="el_headerHorizontalPadding" format="integer|dimension"/>
    </declare-styleable>

</resources>